
class SummarySample {
  
  int playerID;
  int handedness;
  float f1,f2,f3,f4,f5;
  float timeStamp;
  
  SummarySample(int playerID, int handedness, float f1, float f2, float f3, float f4, float f5, float time)
  {
    this.playerID = playerID;
    this.handedness = handedness;
    this.f1 = f1;
    this.f2 = f2;
    this.f3 = f3;
    this.f4 = f4;
    this.f5 = f5;
    this.timeStamp = time;
  }
  
}
